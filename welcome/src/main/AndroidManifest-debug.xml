<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qxj.welcome" >
    <!-- To access Google+ APIs: -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.qxj.welcome.CustomApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
    <activity android:name="com.qxj.welcome.MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

    <activity android:name="com.qxj.welcome.activity.SecondActivity" />
    <activity android:name="com.qxj.welcome.activity.CustomGroupActivity" />
    <activity android:name="com.qxj.welcome.web.URLReceiveActivity">
        <intent-filter>

            <!-- host、scheme 字段，点击url会根据这两个字段调起本地的Activity -->
            <data
                android:host="111"
                android:scheme="arouter" />

            <action android:name="android.intent.action.VIEW" />

            <category android:name="android.intent.category.DEFAULT" />
            <category android:name="android.intent.category.BROWSABLE" />
        </intent-filter>
    </activity>
    <activity android:name="com.qxj.welcome.web.URLWebActivity" />
    <activity android:name="com.qxj.welcome.web.URLWebTwoActivity"></activity>
    </application>
</manifest>
