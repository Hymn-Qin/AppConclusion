<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qxj.welcome" >
    <!-- To access Google+ APIs: -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application>

    <activity android:name=".activity.SecondActivity" />
    <activity android:name=".activity.CustomGroupActivity" />
    <activity android:name=".web.URLReceiveActivity">
        <intent-filter>

            <!-- host、scheme 字段，点击url会根据这两个字段调起本地的Activity -->
            <data
                android:host="111"
                android:scheme="arouter" />

            <action android:name="android.intent.action.VIEW" />

            <category android:name="android.intent.category.DEFAULT" />
            <category android:name="android.intent.category.BROWSABLE" />
        </intent-filter>
    </activity>
    <activity android:name=".web.URLWebActivity" />
    <activity android:name=".web.URLWebTwoActivity">

    </activity>
    </application>
</manifest>
